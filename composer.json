{
    "name": "contrainteractive/wp-laravel-login",
    "description": "Allows a user to login to a Laravel application using an existing WordPress hashed password.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/auth": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "ContraInteractive\\WpLaravelLogin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ContraInteractive\\WpLaravelLogin\\Providers\\WpLoginServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Tim Shultis",
            "email": "alloyking@gmail.com"
        }
    ]
}